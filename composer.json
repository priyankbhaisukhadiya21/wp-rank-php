{
    "name": "wp-rank/php-version",
    "description": "WP-Rank - WordPress Site Rankings in PHP",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.8",
        "vlucas/phpdotenv": "^5.6",
        "ramsey/uuid": "^4.7",
        "vipnytt/robotstxtparser": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "WPRank\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WPRank\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public",
        "crawl": "php bin/crawl_worker.php",
        "discover": "php bin/discover_daily.php",
        "rank": "php bin/recompute_ranks.php"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}