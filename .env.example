# Environment Configuration
APP_ENV=local
APP_DEBUG=true

# Database Configuration
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=global_wp_rank
DB_USER=root
DB_PASS=rootpass

# API Configuration  
ADMIN_TOKEN=dev-admin-token-change-in-production
PSI_API_KEY=your_google_pagespeed_api_key_here

# Crawler Configuration
CRAWL_CONCURRENCY=1
REQUEST_TIMEOUT_SEC=8
PER_DOMAIN_RPS=1
MAX_DOMAINS_PER_RUN=50

# External API Keys (Optional)
GOOGLE_SEARCH_API_KEY=
GOOGLE_SEARCH_ENGINE_ID=
BING_API_KEY=

# Rate Limiting
DISCOVERY_DELAY_SECONDS=2
MAX_PLUGIN_EVIDENCE=20

# Security
CORS_ORIGINS=http://localhost:8000,http://localhost:3000